var switch_clock : bool = false

func _on_Timer_timeout() -> void:
	value -= 1
	switch_clock = !switch_clock
	
	if switch_clock:
		$\"../../../clock1\".play()
	else:
		$\"../../../clock2\".play()

func _on_AnimationPlayer_animation_finished(anim_name):
	if anim_name == \"entrada\":
		$Timer.start()
