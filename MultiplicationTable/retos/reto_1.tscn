script/source = "extends Control

var savedata : Savedata = Savedata.new()
var objetivos : Objetivos = Objetivos.new()

var matrix = [[],[],[],[],[],[],[],[],[]]

var x : int = 0
var y : int = 0
var result : int = 0

var used = [[0,0]]

func matrix_init() -> void:
	var count : int = 0
	
	var tused = [0]
	var ttemp = 0
	
	# load matrix
	for y in range(9):
		for x in range(9):
			matrix[y].append(get_node(\"Control/VBoxContainer/Control/Matrix/\" + String(count)))
			count += 1
	
	# Update Top values
	for x in range(1, 9):
		while ttemp in tused:
			ttemp = (randi() % 8) + 2
		
		matrix[0][x].text = String(ttemp)
		tused.append(ttemp)
	
	# update left values
	tused.clear()
	for y in range(1, 9):
		while ttemp in tused:
			ttemp = (randi() % 8) + 2
		
		matrix[y][0].text = String(ttemp)
		tused.append(ttemp)
	
	# clear zero values
	for y in range(1,9):
		for x in range(1,9):
			matrix[y][x].text = \"\"

func matrix_clear_color() -> void:
	for y in range(1,9):
		for x in range(1,9):
			matrix[y][x].set(\"custom_styles/normal\", preload(\"res://retos/Resources/clear.tres\"))
			matrix[y][x].set(\"custom_colors/font_color\", Color(\"555555\"))

func fallaste() -> void:
	$Control/VBoxContainer/resultado.text = \"\"
	$Control/VBoxContainer/TextureProgress/Timer.stop()
	$error.play()
	$AnimationPlayer.play(\"fallaste\")
	savedata.load_data()
	savedata.r1[0] += 1
	savedata.save_data()
	yield(get_tree().create_timer(4), 'timeout')
	get_tree().change_scene(\"res://scenes/main_menu_black.tscn\")

func _notification(what):
	if what == MainLoop.NOTIFICATION_WM_GO_BACK_REQUEST:
		var animation = preload(\"res://animations/alerta_noSePuedeAbandonar.tscn\").instance()
		animation.z_index = 500
		add_child(animation)

func _ready() -> void:
	$language.apply_language()
	randomize()
	matrix_init()
	update_values()

func _process(delta):
	if matrix[y][x].text.length() == 0:
		$Control/VBoxContainer/teclado/ok.disabled = true
	else:
		$Control/VBoxContainer/teclado/ok.disabled = false
 
func update_values():
	print(used.size())
	if used.size() > 64:
		$Control/VBoxContainer/TextureProgress/Timer.stop()
		savedata.load_data()
		savedata.r1[1] = true
		savedata.nivel += 1
		savedata.save_data()
		objetivos.check_objetivo()
		
		#Animation
		matrix_clear_color()
		
		for y in range(1,9):
			for x in range(1,9):
				matrix[y][x].set(\"custom_styles/normal\", preload(\"res://retos/Resources/selected_green.tres\"))
				matrix[y][x].set(\"custom_colors/font_color\", Color(\"FFFFFF\"))
				
			$plus.play()
			for x in range(3):
				var plus = preload(\"res://animations/Plus.tscn\").instance()
				plus.position.x = get_viewport_rect().size.x / rand_range(1.5, 3)
				plus.position.y = get_viewport_rect().size.y / rand_range(1.5, 3)
				plus.z_index = 500
				add_child(plus)
				yield(get_tree().create_timer(0.1), 'timeout')
			
			for x in range(1,9):
				matrix[y][x].set(\"custom_styles/normal\", preload(\"res://retos/Resources/clear.tres\"))
				matrix[y][x].text = ''
				
		$AnimationPlayer.play(\"victoria\")
		$victoria_sound.play()
		return
		
	var i = 1
	var j = 1
	matrix_clear_color()
	
	while [x, y] in used:
		x = (randi() % 8) + 1
		y = (randi() % 8) + 1
	
	used.append([x, y])
	
	result = int(matrix[y][0].text) * int(matrix[0][x].text)
	matrix[y][x].set(\"custom_styles/normal\", preload(\"res://retos/Resources/selected_green.tres\"))
	matrix[y][x].set(\"custom_colors/font_color\", Color(\"ffffff\"))
	
	# Animation fill overlight
	while i < y or j < x:
		if not i >= y:
			matrix[i][x].set(\"custom_styles/normal\", preload(\"res://retos/Resources/selected_blue_overlight.tres\"))
		
		if not j >= x:
			matrix[y][i].set(\"custom_styles/normal\", preload(\"res://retos/Resources/selected_green_overlight.tres\"))
		
		yield(get_tree().create_timer(0.1), 'timeout')
		
		i += 1
		j += 1

func _on_TextureProgress_value_changed(value: float) -> void:
	if value == 0:
		fallaste()

func _on_teclado_keyboad_pressed(button):
	
	$tap.play()
	
	if button == \"del\" and matrix[y][x].text.length() > 0:
		matrix[y][x].text = matrix[y][x].text.substr(0, matrix[y][x].text.length() - 1)
		
	elif button != \"ok\" and button != \"del\" and matrix[y][x].text.length() < 2:
		matrix[y][x].text += button
		
	elif button == \"ok\" and matrix[y][x].text.length() > 0:
		$Control/VBoxContainer/TextureProgress/Timer.stop()
		
		if result == int(matrix[y][x].text):
			$Control/VBoxContainer/TextureProgress.value = $Control/VBoxContainer/TextureProgress.max_value
			$Control/VBoxContainer/TextureProgress/Timer.start()
			update_values()
			#Animation
			for x in range(3):
				$plus.play()
				var plus = preload(\"res://animations/Plus.tscn\").instance()
				plus.position.x = get_viewport_rect().size.x / rand_range(1.5, 3)
				plus.position.y = get_viewport_rect().size.y / rand_range(1.5, 3)
				plus.z_index = 500
				add_child(plus)
				yield(get_tree().create_timer(0.1), 'timeout')
				
		else:
			fallaste()

func _on_AnimationPlayer_animation_finished(anim_name):
	if anim_name == 'victoria':
		get_tree().change_scene(\"res://scenes/main_menu_black.tscn\")"

