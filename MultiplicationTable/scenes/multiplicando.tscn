		get_parent().get_node(\"Victoria/VBoxContainer/Control/HBoxContainer/fallidos_label\").text = \"Mistakes:\"
		get_parent().get_node(\"Victoria/VBoxContainer/toca_para_continuar\").text = \"Touch to continue\""

