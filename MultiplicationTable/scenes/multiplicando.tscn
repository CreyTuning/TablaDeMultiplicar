script/source = "extends Node

var language : Language = Language.new()
export var lang = 'es'

func _ready() -> void:
	language.load_data()
	lang = language.language
	
export master func apply_language():
	language.load_data()
	
	if language.language == 'en':
		get_parent().get_node(\"Control/VBoxContainer/botones_opciones/salir\").text = \"Exit\"
		get_parent().get_node(\"Control/VBoxContainer/botones_opciones/ayuda\").text = \"Help\"
