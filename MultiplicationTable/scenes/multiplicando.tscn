script/source = "extends Control

var savedata = Savedata.new()
var multiplicando = Multiplicando.new()

var puntos_inicial : int = 0
var bono : int = 0
var correctos : int = 0
var fallidos : int = 0

func _notification(what):
	if what == MainLoop.NOTIFICATION_WM_GO_BACK_REQUEST:
		_on_salir_pressed()

func _ready() -> void:
	$language.apply_language()
	savedata.load_data()
	puntos_inicial = savedata.puntos
	$Control/VBoxContainer/tabla/valor1.text = String(savedata.tabla_actual)
	$Control/VBoxContainer/tabla/valor2.text = String(multiplicando.get_number())
	$Control/VBoxContainer/botones_opciones/ayuda/count.text = String(savedata.ayudas)

func _process(delta: float) -> void:
	if $Control/VBoxContainer/resultado.text == \"\":
		$Control/VBoxContainer/teclado/del.disabled = true
		$Control/VBoxContainer/teclado/ok.disabled = true
	else:
		$Control/VBoxContainer/teclado/del.disabled = false
		$Control/VBoxContainer/teclado/ok.disabled = false

func _on_salir_pressed() -> void:
	get_tree().change_scene(\"res://scenes/main_menu.tscn\")

func _on_ok_button_down() -> void:
	savedata.load_data()
	# Si la respuesta es correcta
	if int($Control/VBoxContainer/resultado.text) == savedata.tabla_actual * multiplicando.get_current():
		# Verificar que todo este completado y evitar mostrar -1 en valor2
		if multiplicando.get_number() != -1:
			$Control/VBoxContainer/tabla/valor2.text = String(multiplicando.get_current())
		$TextureProgress.value += 1

		# Actualizar datos por tabla
		match savedata.tabla_actual:
			2: savedata.t2[0] += 1
			3: savedata.t3[0] += 1
			4: savedata.t4[0] += 1
			5: savedata.t5[0] += 1
			6: savedata.t6[0] += 1
			7: savedata.t7[0] += 1
			8: savedata.t8[0] += 1
			9: savedata.t9[0] += 1

		# Update local var
		correctos += 1

		# update puntos
		if savedata.puntos < 4000000000:
			if savedata.tabla_actual < (savedata.nivel + 2):
				savedata.puntos += 1
			else:
				savedata.puntos += 5 + (savedata.nivel * savedata.tabla_actual)
			savedata.save_data()

		# Animation
		var plus = preload(\"res://animations/Plus.tscn\").instance()
		plus.position.x = get_viewport_rect().size.x / 2
		plus.position.y = get_viewport_rect().size.y / 5
		plus.z_index = 500
		$Control/VBoxContainer/resultado.add_child(plus)

		# Audio
		$plus.play()

		# Verificar que se ha terminado
		if multiplicando.completed():
			savedata.load_data()

			match savedata.tabla_actual:
				2: savedata.t2[2] += 1
				3: savedata.t3[2] += 1
				4: savedata.t4[2] += 1
				5: savedata.t5[2] += 1
				6: savedata.t6[2] += 1
				7: savedata.t7[2] += 1
				8: savedata.t8[2] += 1
				9: savedata.t9[2] += 1

			# Bono update value
			if (savedata.puntos - puntos_inicial) < 0:
				bono = 0
			else:
				bono = int((savedata.puntos - puntos_inicial) * rand_range(0.1, 0.3))

			savedata.puntos += bono
			puntos_inicial += bono

			savedata.save_data()
			$Control/VBoxContainer/tabla/valor1.text = \"0\"
			$Control/VBoxContainer/tabla/valor2.text = \"0\"
			$Control/VBoxContainer/resultado.text = \"\"
			$transition.visible = true

			yield(get_tree().create_timer(0.25), 'timeout')
			victoria()

	else: # Si la respuesta no es correcta
		match savedata.tabla_actual:
			2: savedata.t2[1] += 1
			3: savedata.t3[1] += 1
			4: savedata.t4[1] += 1
			5: savedata.t5[1] += 1
			6: savedata.t6[1] += 1
			7: savedata.t7[1] += 1
			8: savedata.t8[1] += 1
			9: savedata.t9[1] += 1

		# Update local var
		fallidos += 1

		# update puntos
		if savedata.puntos > 0:
			savedata.puntos -= (10 + (savedata.nivel * savedata.tabla_actual))

			if savedata.puntos < 0:
				savedata.puntos = 0

			savedata.save_data()

		# Animation
		var less = preload(\"res://animations/Less.tscn\").instance()
		less.position.x = get_viewport_rect().size.x / 2
		less.position.y = get_viewport_rect().size.y / 5
		less.z_index = 500
		$Control/VBoxContainer/resultado.add_child(less)

		# Audio
		$error.play()

	$Control/VBoxContainer/resultado.text = \"\"

func _on_ayuda_pressed() -> void:
	if savedata.ayudas > 0:
		savedata.ayudas -= 1
		$Control/VBoxContainer/botones_opciones/ayuda.text = String(savedata.tabla_actual * multiplicando.get_current())
		$Control/VBoxContainer/botones_opciones/ayuda/count.text = String(savedata.ayudas)
		savedata.save_data() 
		$Control/VBoxContainer/botones_opciones/ayuda.disabled = true
		yield(get_tree().create_timer(1), \"timeout\")
		$Control/VBoxContainer/botones_opciones/ayuda.disabled = false
		
		if $language.lang == \"es\":
			$Control/VBoxContainer/botones_opciones/ayuda.text = \"Ayuda\"
		elif $language.lang == \"en\":
			$Control/VBoxContainer/botones_opciones/ayuda.text = \"Help\"
	else:
		$Control/VBoxContainer/botones_opciones/ayuda.text = \".\"
		$Control/VBoxContainer/botones_opciones/ayuda.disabled = true
		yield(get_tree().create_timer(0.33), \"timeout\")
		$Control/VBoxContainer/botones_opciones/ayuda.text += \".\"
		yield(get_tree().create_timer(0.33), \"timeout\")
		$Control/VBoxContainer/botones_opciones/ayuda.text += \".\"
		yield(get_tree().create_timer(0.33), \"timeout\")
		$Control/VBoxContainer/botones_opciones/ayuda.disabled = false
		
		if $language.lang == \"es\":
			$Control/VBoxContainer/botones_opciones/ayuda.text = \"Ayuda\"
		elif $language.lang == \"en\":
			$Control/VBoxContainer/botones_opciones/ayuda.text = \"Help\"

func victoria() -> void:
	var fracaso : bool = savedata.puntos - puntos_inicial < 0

	$Victoria/VBoxContainer/Control/HBoxContainer/correctos.text = String(correctos)
	$Victoria/VBoxContainer/Control/HBoxContainer/fallidos.text = String(fallidos)
	$Victoria/VBoxContainer/bonos.text = \"0\"
	$Victoria/VBoxContainer/puntos.text = \"0\"
	$Victoria/VBoxContainer/total.text = \"0\"

	# Verificar tipo de animacion
	if not fracaso:
		if $language.lang == \"es\":
			$Victoria/VBoxContainer/VBoxContainer/titulo.text = \"Completado\"
		elif $language.lang == \"en\":
			$Victoria/VBoxContainer/VBoxContainer/titulo.text = \"Completed\"
			
		$Victoria/Victoria_AnimationPlayer.play(\"init\")
	else:
		if $language.lang == \"es\":
			$Victoria/VBoxContainer/VBoxContainer/titulo.text = \"Fallaste\"
		elif $language.lang == \"en\":
			$Victoria/VBoxContainer/VBoxContainer/titulo.text = \"Failed\"
		$Victoria/Victoria_AnimationPlayer.play(\"init_red\")

	# Point Animation
	for x in range(abs(savedata.puntos - puntos_inicial)):
		if not fracaso:
			if x % 10 == 0:
				var plus = preload(\"res://animations/Plus.tscn\").instance()
				plus.position.x = (get_viewport().size.x / 2) + rand_range(-100, 100)
				plus.position.y = $Victoria/VBoxContainer/puntos.rect_global_position.y + rand_range(-100, 100)
				add_child(plus)
				$plus.play()

			$Victoria/VBoxContainer/puntos.text = String(int($Victoria/VBoxContainer/puntos.text) + 1)
			$Victoria/VBoxContainer/total.text = String(int($Victoria/VBoxContainer/total.text) + 1)

			yield(get_tree().create_timer(0.0025), \"timeout\")
		else:
			if x % 10 == 0:
				var less = preload(\"res://animations/Less.tscn\").instance()
				less.position.x = (get_viewport().size.x / 2) + rand_range(-100, 100)
				less.position.y = $Victoria/VBoxContainer/puntos.rect_global_position.y + rand_range(-100, 100)
				add_child(less)
				$error.play()

			$Victoria/VBoxContainer/puntos.text = String(int($Victoria/VBoxContainer/puntos.text) - 1)
			$Victoria/VBoxContainer/total.text = String(int($Victoria/VBoxContainer/total.text) - 1)

			yield(get_tree().create_timer(0.0025), \"timeout\")

	# Bonus Animation
	yield(get_tree().create_timer(0.5), \"timeout\")

	for x in range(bono):
		if x % 10 == 0:
			var plus = preload(\"res://animations/Plus.tscn\").instance()
			plus.position.x = (get_viewport().size.x / 2) + rand_range(-100, 100)
			plus.position.y = $Victoria/VBoxContainer/bonos.rect_global_position.y + rand_range(-100, 100)
			add_child(plus)
			$plus.play()

		$Victoria/VBoxContainer/bonos.text = String(int($Victoria/VBoxContainer/bonos.text) + 1)
		$Victoria/VBoxContainer/total.text = String(int($Victoria/VBoxContainer/total.text) + 1)

		yield(get_tree().create_timer(0.005), \"timeout\")"

